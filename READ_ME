from django.contrib.sites.models import Site
from django.contrib.flatpages.models import FlatPage
our_site=Site.objects.all()[0]
#already did these:
#our_site.name='POC'
#our_site.domain='pp3.cloud'
#our_site.save()
#about_page=FlatPage.objects.create(url='/about/',title='<p>whatever</p>'
#(no need to save about_page)
#need relationship with our_site:
#about_page=sites.add(our_sites) already did that
#(no need to save about_page)
about_page=FlatPage.objects.all()[0]
from django.core.urlresolvers import reverse_lazy, reverse
about_page.get_absolute_url()
reverse('django.contrib.flatpages.views.flatpage',kwargs={'url':about_page.url})

about_page.content="<p>    Possible Political Prisoners (www.pp3.cloud) is a database and web site" 
about_page.content+=("  dedicated to tracking political prisoners. ")
about_page.content+=("  Currently we have about 16,000 worldwide. ")
about_page.content+=("<p>     In a glance, one can compare the quantities attributed to various countries, examine the list of prisoners in any country and examine the details of any prisoner.")

about_page.content+=("<p>     Also anybody can register and log in, then:")
about_page.content+=("  1) create a record for a possible political prisoner;")
about_page.content+=("  2) attach unlimited newslinks to any prisoner;")
about_page.content+=("  3) upload a photo of any prisoner;")
about_page.content+=("  4) blog about any prisoner. ")
about_page.content+=("  The newlinks and blogs will appear on the detail page of both the prisoner and the country.")
about_page.save()













