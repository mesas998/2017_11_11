import smtplib
fromaddr = 'michael.sweeney303@gmail.com'
toaddrs  = 'michael.sweeney303@gmail.com'
msg = 'Why,Oh why!'
username = 'michael.sweeney303'
password = 'fdrdfdt32~'
#erver = smtplib.SMTP('smtp.gmail.com:465') #SSL
server = smtplib.SMTP('smtp.gmail.com:587') #TLS
print("11")
server.starttls()
print('15')
server.ehlo()
print("22")
server.login(username,password)
print("33")
server.sendmail(fromaddr, toaddrs, msg)
print("44")
server.quit()
